{
	"lang": "pt",
	"prefix": ".",
	"timezone": "America/Sao_Paulo",
	"ai": {
		"gemini_chain": [
			"gemini-2.0-flash-lite",
			"gemini-2.0-flash",
			"gemini-2.5-flash",
			"gemini-2.5-pro"
		],
		"gemini_pro": "gemini-2.5-pro-preview-05-06"
	},
	"cache": {
		"users": 200,
		"groups": 200,
		"dmMsgs": 10,
		"groupMsgs": 200
	},
	"runner": {
		"tempFolder": "conf/temp",
		"ytdlp": "conf/venv/bin/yt-dlp",
		"py": {
			"cmd": [
				"conf/venv/bin/python3"
			],
			"ext": "py"
		},
		"lua": {
			"cmd": [
				"luajit"
			],
			"ext": "lua"
		},
		"node": {
			"cmd": [
				"node"
			],
			"ext": "js"
		},
		"deno": {
			"cmd": [
				"deno run -A"
			],
			"ext": "ts"
		},
		"bun": {
			"cmd": [
				"bun run"
			],
			"ext": "ts"
		},
		"fish": {
			"cmd": [
				"fish"
			],
			"ext": "sh"
		},
		"zsh": {
			"cmd": [
				"zsh"
			],
			"ext": "sh"
		},
		"cpp": {
			"cmd": [
				"g++ -o conf/temp/main ",
				"./conf/temp/main #"
			],
			"ext": "cpp",
			"triggers": [{
				"notIncludes": ["int main"],
				"template": "int main() { {{code}} }"
			}, {
				"notIncludes": ["#include <iostream>"],
				"template": "#include <iostream>\n{{code}}"
			}]
		},
		"rs": {
			"cmd": [
				"rustc -o conf/temp/main",
				"./conf/temp/main"
			],
			"ext": "rs",
			"triggers": [{
				"notIncludes": ["fn main"],
				"template": "fn main() { {{code}} }"
			}]
		},
		"eval": {
			"cmd": [],
			"triggers": [{
				"includes": ["await"],
				"template": "(async () => { {{code}} })()"
			}]
		}
	}
}
