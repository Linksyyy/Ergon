{
	"lang": "pt",
	"prefix": ".",
	"timezone": "America/Sao_Paulo",
	"ai": {
		"gemini_chain": [
			"gemini-2.0-flash-lite",
			"gemini-2.0-flash",
			"gemini-2.5-flash",
			"gemini-2.5-pro"
		],
		"gemini_pro": "gemini-2.5-pro-preview-05-06"
	},
	"cache": {
		"users": 200,
		"groups": 200,
		"dmMsgs": 10,
		"groupMsgs": 200
	},
	"runner": {
		"tempFolder": "conf/temp/",
		"ytdlp": "conf/venv/bin/yt-dlp",
		"py": {
			"cmd": [
				"conf/venv/bin/python3"
			],
			"ext": "py",
			"templates": {}
		},
		"lua": {
			"cmd": [
				"luajit"
			],
			"ext": "lua",
			"templates": {}
		},
		"node": {
			"cmd": [
				"node"
			],
			"ext": "js",
			"templates": {}
		},
		"deno": {
			"cmd": [
				"deno run -A"
			],
			"ext": "ts",
			"templates": {}
		},
		"bun": {
			"cmd": [
				"bun run"
			],
			"ext": "ts",
			"templates": {}
		},
		"fish": {
			"cmd": [
				"fish"
			],
			"ext": "sh",
			"templates": {}
		},
		"zsh": {
			"cmd": [
				"zsh"
			],
			"ext": "sh",
			"templates": {}
		},
		"cpp": {
			"cmd": [
				"g++ -o conf/temp/main ",
				"./conf/temp/main #"
			],
			"ext": "cpp",
			"templates": {
				"main": "#include <iostream>\n int main() {\n//__CODE__//\n}"
			}
		},
		"rust": {
			"cmd": [
				"rustc -o conf/temp/main",
				"./conf/temp/main"
			],
			"ext": "rs",
			"templates": {
				"main": "fn main() {\n//__CODE__//\n}"
			}
		}
	}
}