{
	"bot": {
		"link": "dsc.gg/wallebot",
		"region": {
			"timezone": "America/Sao_Paulo",
			"logLanguage": "pt"
		}
	},
	"db": {
		"user": {
			"prefix": ".",
			"language": "pt",
			"cacheLimit": 500,
			"msgsLimit": 200
		},
		"group": {
			"msgsLimit": 200
		}
	},
	"api": {
		"aiModel": {
			"gpt": "gpt-3.5-turbo",
			"gemini": "gemini-2.0-flash",
			"geminiPro": "gemini-2.5-pro-exp-03-25"
		}
	},
	"sticker": {
		"packName": [
			"꒷︶꒷꒦ Sticker ꒷꒦︶꒷",
			"╰───────────╮\n",
			"╭︰꒰👑꒱・Author:",
			"│︰꒰🤖꒱・Bot:",
			"│︰꒰❤️꒱・Owner:",
			"│︰꒰❓꒱・Support:",
			"│︰꒰📅꒱・Date:",
			"╰︰꒰⛄꒱・Group:"
		],
		"author": [
			"꒷︶꒷꒦ Metadata ꒷꒦︶꒷",
			"╭────────────╯\n",
			"︰{username}",
			"︰Wall-E ⚡",
			"︰Sunf3r",
			"︰{link}",
			"︰{date}",
			"︰{group}"
		]
	},
	"runner": {
		"tempFolder": "conf/temp",
		"py": {
			"cmd": [
				"conf/venv/bin/python3"
			],
			"ext": "py"
		},
		"lua": {
			"cmd": [
				"luajit"
			],
			"ext": "lua"
		},
		"node": {
			"cmd": [
				"node"
			],
			"ext": "js"
		},
		"deno": {
			"cmd": [
				"deno run -A"
			],
			"ext": "ts"
		},
		"bun": {
			"cmd": [
				"bun run"
			],
			"ext": "ts"
		},
		"zsh": {
			"cmd": [
				"zsh"
			],
			"ext": "sh"
		},
		"cpp": {
			"cmd": [
				"g++ -o conf/temp/main ",
				"./conf/temp/main #"
			],
			"ext": "cpp"
		}
	}
}
